{
  "name": "virtual-backlog",
  "version": "1.0.0",
  "description": "A full-stack MERN React app for users to store and display their entire videogame collections on animated 3D bookshelves.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwkeena/virtual-backlog.git"
  },
  "author": "Justin Keena, David Ross Banville",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jwkeena/virtual-backlog/issues"
  },  
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "mongoose": "^5.3.16",
    "if-env": "^1.0.4"
  },
  "homepage": "https://github.com/jwkeena/virtual-backlog#readme"
}
